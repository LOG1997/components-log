{
    "name": "components-log",
    "version": "0.0.5",
    "description": "A Vue3's component for generating CSS Mesh Gradients",
    "main": "index.js",
    "module": "packages/logui/lib/components/index.js",
    "typings": "packages/logui/es/components/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,vue}": [
            "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix --quiet ./",
            "stylelint --fix \"packages/components/src/**/*.{css,less}\""
        ]
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "pnpm -C play dev",
