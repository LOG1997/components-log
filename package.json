        "docs:dev": "vitepress dev doc",
        "docs:build": "vitepress build doc",
        "docs:preview": "vitepress preview"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/LOG1997/components-log"
    },
    "homepage": "https://log1997.github.io/components-log/#/",
    "keywords": [
        "gradient",
        "vue3",
        "color",
        "mesh gradient",
        "mesh",
        "component",
